{
    "name": "phpbadge/phpbadge",
    "description": "A PHP library to create badges.",
    "keywords": ["badge", "badges", "pose", "button", "phpbadge"],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "ext-gd": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "phpstan/phpstan": "^0.11.2",
        "phpstan/phpstan-phpunit": "^0.11.0",
        "phpstan/phpstan-strict-rules": "^0.11.0"
    },
    "autoload": {
        "psr-4": {
            "PHP\\Badge\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHP\\BadgeTest\\": "tests/BadgeTest",
            "PHP\\BadgeTestAsset\\": "tests/BadgeTestAsset"
        }
    },
    "authors": [
        {
            "name": "Walter Tamboer",
            "homepage": "http://www.waltertamboer.nl",
            "role": "Developer"
        }
    ]
}
